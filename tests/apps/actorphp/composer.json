{
    "name": "dapr/actorphp",
    "description": "A simple e2e test actor",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Landers",
            "email": "landers.robert@gmail.com"
        }
    ],
    "require": {
        "dapr/php-sdk": "dev-main",
        "monolog/monolog": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Test\\": "src"
        }
    },
    "scripts": {
        "start-env": [
            "docker build -t php-sdk-test .",
            "docker run --rm --name=phpsdktest -v $(pwd):/app -p 3000:3000 php-sdk-test"
        ]
    }
}
